function Tang()
{
	var a=document.getElementById('value').value;
	a=Number(a);
	if ((a>=25)==true)
	{
		var b=a-25;
		document.getElementById('change').innerHTML="Thank you for buying please come again here's your change: "+b;
		alert('Enjoy your Coke')
	}
	else
	{
		var b=25-a;
		alert('Insufficient Amount!!');
		document.getElementById('change').innerHTML="Your money needs "+b+" more to avail this one";
	}
}
